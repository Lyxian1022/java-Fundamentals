public class interfac {

	public static void main(String[] args) {
		Apear ap=new Apear();
		ap.growapple();
		ap.growpear();
	}
}
interface Apple{
	public void growapple();
}
interface Pear{
	public void growpear();
}
class Apear implements Apple,Pear{
	public void growapple() {
		System.out.println("å†œå¤«æœå›­è‹¹æœ1å·");
	}
	public void growpear() {
		System.out.println("å†œå¤«æœå›­ğŸ1å·");
	}
}
